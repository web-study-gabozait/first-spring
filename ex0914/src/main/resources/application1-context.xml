<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="hello" class = "studySpring2022.studySpring2022.Hello">
        <property name="person" ref="mary"></property>
    </bean>

    <bean id="smith" class = "studySpring2022.studySpring2022.Person">
        <property name="name" value="Mr.smith"></property>
    </bean>

    <bean id="mary" class = "studySpring2022.studySpring2022.Person">
        <property name="name" value="Mr.mary"></property>
    </bean>


</beans>